{
  "collectionName": "components_ui_custom_links",
  "info": {
    "displayName": "CustomLink",
    "icon": "cursor"
  },
  "options": {},
  "attributes": {
    "children": {
      "type": "string",
      "required": true
    },
    "href": {
      "type": "string",
      "required": true
    },
    "target": {
      "type": "enumeration",
      "enum": [
        "_blank",
        "_self",
        "_parent",
        "_top"
      ]
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "primary",
        "secondary",
        "transparent"
      ]
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "small",
        "medium",
        "large"
      ],
      "default": "medium"
    }
  }
}
